<!-- Top header, include this header in your page-->
<div class="container-fluid py-2 main-div bg-warning">
    <div class="d-flex justify-content-between align-items-center">
        <img src="data:image/png;base64,"  width="40" height="40" id="homepage-btn" role="button"></img>

        <!-- Search bar in the middle -->
        <form id="search-form" action="/items/search" method="get" class="w-50">
            <div class="input-group flex-nowrap rounded-pill">
                <input id="inpSearch" name="name" class="form-control" type="search" placeholder="Search" aria-label="Search">
                <button type="button" id="searchButton" class="btn btn-secondary">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" class="bi bi-search" fill="white" viewBox="0 0 16 16">
                        <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001q.044.06.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1 1 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0"/>
                    </svg>
                </button>
            </div>
        </form>

        <div>
            <!-- Button to shopping cart -->
            <button type="button" id="cart-btn" class="btn">
                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="white" class="bi bi-cart" viewBox="0 0 16 16">
                    <path d="M0 1.5A.5.5 0 0 1 .5 1H2a.5.5 0 0 1 .485.379L2.89 3H14.5a.5.5 0 0 1 .491.592l-1.5 8A.5.5 0 0 1 13 12H4a.5.5 0 0 1-.491-.408L2.01 3.607 1.61 2H.5a.5.5 0 0 1-.5-.5M3.102 4l1.313 7h8.17l1.313-7zM5 12a2 2 0 1 0 0 4 2 2 0 0 0 0-4m7 0a2 2 0 1 0 0 4 2 2 0 0 0 0-4m-7 1a1 1 0 1 1 0 2 1 1 0 0 1 0-2m7 0a1 1 0 1 1 0 2 1 1 0 0 1 0-2"/>
                </svg>
            </button>

            <!-- Button to personal page -->
            <button type="button" id="userpage-btn" class="btn">
                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="white" class="bi bi-person-fill" viewBox="0 0 16 16">
                    <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6"/>
                </svg>
            </button>
        </div>
    </div>
</div>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script>
    $(document).ready(function() {
        $("#homepage-btn").click(function(){
            location.href = '/items';
        });

        $("#userpage-btn").click(function() {
            location.href = '/user_page';
        });

        $("#cart-btn").click(function(){
            location.href = '/cart';
        });

        $("#searchButton").click(function() {
            $("#search-form").submit();
        });
    });
</script>